{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "i18n:extract:fr": "ngx-translate-extract --input ./src --output ./src/assets/i18n/fr.json -s -n --format namespaced-json",
    "i18n:extract:en": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json -s -n --format namespaced-json",
    "i18n:extract:all": "npm run i18n:extract:fr && npm run i18n:extract:en",
    "dev:ssr": "ng run Blog:serve-ssr",
    "serve:ssr": "node dist/Blog/server/main.js",
    "build:ssr": "ng build && ng run Blog:server",
    "prerender": "ng run Blog:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-brands-svg-icons": "^6.1.0",
    "@fortawesome/free-regular-svg-icons": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@nguniversal/express-engine": "^13.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "express": "^4.15.2",
    "ngx-typed-js": "^2.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@bartholomej/ngx-translate-extract": "^8.0.2",
    "@nguniversal/builders": "^13.1.1",
    "@tailwindcss/forms": "^0.5.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.7",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.2"
  }
}
